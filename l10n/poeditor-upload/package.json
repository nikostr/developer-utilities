{
  "name": "zetkin-poeditor-upload",
  "version": "1.1.0",
  "description": "Read YAML data from filesystem and publish it to the poeditor.com website",
  "repository": "https://github.com/zetkin/developer-utilities",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "showEnglishTerms": "node . | jq '[ .allTerms.en[] | { term: .term } ]'",
    "showLanguages": "node . | jq --raw-output '.allTerms | keys | join(\" \")' "
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "flat": "^4.1.0",
    "node-yaml": "^3.0.3",
    "recursive-readdir": "^2.1.0"
  }
}
